<%- include("partials/header", { bot, user, path, title: "Home" }) %>
<div class="wrapper">
  <div class="page-header">
    <div class="content">
      <div class="container">
          <div class="card">
            <h4 class="card-title"><%= guild.name %></h4>
            <div class="card-body">
              <form method="POST">
                <div class="form-group">
                  <label for="<%= settings.prefix %>">Prefix</label>
                  <input type="text" class="form-control" id="<%= settings.prefix %>" name="prefix" value="<%= settings.prefix %>" placeholder="Enter prefix">
                </div>
                <button type="submit" class="btn btn-primary">Save Settings</button>
              </form>
            </div>
        </div>
      </div> 
  </div>
</div>
<%- include("partials/footer") %>
